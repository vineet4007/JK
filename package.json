      "@nestjs/platform-express": "^10.0.0",
      "@nestjs/typeorm": "^10.0.0",
      "bcrypt": "^5.1.0",
      "class-transformer": "^0.5.1",
      "class-validator": "^0.14.0",
      "dotenv": "^16.0.3",
      "passport": "^0.6.0",
      "passport-jwt": "^4.0.1",
      "pg": "^8.11.0",
